!!!
%html{:xmlns => "http://www.w3.org/1999/xhtml", "xml:lang" => "en", :lang => "en"}
  %head
    :javascript
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-12546091-2']);
        _gaq.push(['_trackPageview']);

        (function() {
          var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
          ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
          var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    %title
      GoGolf -
      = @title
  = csrf_meta_tag
  = stylesheet_link_tag "screen", "own", "jquery-ui", "featurelist", "formtastic"
  = javascript_include_tag :defaults, :ckeditor, 'autocomplete-rails.js', 'featurelist'
  %body
    .container
      .span-24.last
        #banner_top1
      .span-24.last
        #banner_top
          %img{:src => "/images/layout/left_first-04.jpg", :alt => "GoGolf.fi"}
          = render :partial => 'shared/buttons'
          %img{:src => "/images/layout/left_right.jpg", :height => "87", :width => "280", :alt => "GoGolf.fi"}
      .span-24.last
        #navigation
          .span-5
            &nbsp
          .span-19.last
            = render :partial => 'shared/subnavigation'
      .span-24.last
        #banner_top
          %img{:src => "/images/layout/bottom_whole.jpg", :width => "950", :alt => "GoGolf.fi"}
      .span-24.last{:class => "content_all"}
        .span-17{:class => "content_all"}
          .ui-widget

            #main
              = yield 
              - if ENV["RAILS_ENV"] == "development"
                = debug(params)
        .span-7.last{:class => "content_all"}
          - flash.each do |name, msg|
            .notice
              = content_tag :div, msg, :id => "flash_\#{name}"
          =render :partial => 'shared/sidebar'
          =render :partial => 'shared/sidebar2'
