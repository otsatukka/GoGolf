<h3>Käyttäjän <%= @user.name %> kierrokset</h1>

<table>
  <tr>
    <th>Strokes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rounds.each do |round| %>
  <tr>
    <td><%= round.strokes %></td>
    <td><%= link_to 'Show', user_round_path(@user, round) %></td>
    <td><%= link_to 'Edit', edit_user_round_path(@user, round) %></td>
    <td><%= link_to 'Destroy', [@user, round], :method => :delete, :confirm => "Are you sure?" %>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Round', new_user_round_path(@user) %>
