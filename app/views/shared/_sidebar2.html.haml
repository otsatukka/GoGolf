.ui-widget
  .ui-widget-header{:class => "ui-corner-all"}
    Luetuimmat
  .ui-widget-content
    #luetuimmat
      %h4
        Uutiset
        %hr/
      - @top5_posts_of_week.each do |post|
        %h6= link_to post.title, post
        %i
          = link_to post.user.end_user_name, post.user
          |
          = post.unique_impression_count
        %br/
      %h4
        Avaukset
        %hr/
      - @top5_openings_of_week.each do |post|
        %h6= link_to post.title, post
        %i
          = link_to post.user.end_user_name, post.user
          |
          = post.unique_impression_count
        %br/
      
